  "type": "module",
  "bugs": {
    "url": "https://github.com/Pedrojok01/Pedrojok01.github.io/issues",
    "email": "pedrojok@pm.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pedrojok01/Pedrojok01.github.io"
  },
  "dependencies": {
    "bootstrap": "5.3.2"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "3.0.0",
    "autoprefixer": "10.4.16",
    "browser-sync": "2.29.3",
    "chokidar": "3.5.3",
    "concurrently": "^8.2.2",
    "fs-extra": "^11.2.0",
    "postcss": "8.4.32",
    "prettier": "3.1.0",
    "pug": "3.0.2",
    "sass": "1.69.5"
  }
}
