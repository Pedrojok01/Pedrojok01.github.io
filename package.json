{
  "title": "Resume",
  "name": "startbootstrap-resume",
  "version": "7.0.21",
  "scripts": {
    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets && npm run update-root",
    "build:assets": "node scripts/build-assets.js",
    "build:pug": "node scripts/build-pug.js",
    "build:scripts": "node scripts/build-scripts.js",
    "build:scss": "node scripts/build-scss.js",
    "update-root": "node scripts/update-root-index.js",
    "clean": "node scripts/clean.js",
    "start": "npm run build && node scripts/start.js",
    "start:debug": "npm run build && node scripts/start-debug.js",
    "prettier": "prettier --write \"**/*.{js,json,html,css,scss,yml,pug}\" --plugin=\"@prettier/plugin-pug\""
  },
  "description": "Resume is a simple, yet elegant resume and CV theme for Bootstrap 5.",
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template"
  ],
  "homepage": "https://github.com/Pedrojok01/Pedrojok01.github.io",
