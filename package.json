  "contributors": [
    "David Miller (https://davidmiller.io/)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Pedrojok01/Pedrojok01.github.io"
  },
  "dependencies": {
    "bootstrap": "4.5.3"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^2.5.0",
    "autoprefixer": "10.4.14",
    "browser-sync": "2.29.3",
    "chokidar": "3.5.3",
    "concurrently": "5.3.0",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "pug": "3.0.2",
    "sass": "1.62.1",
    "shelljs": "0.8.5"
  }
}
